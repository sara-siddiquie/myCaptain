#include <iostream>
using namespace std;

int main()
{

    int arr[5];

    cout << "Enter the elements of array having 5 elements  " << endl;
    cin >> arr[0] >> arr[1] >> arr[2] >> arr[3] >> arr[4];

    cout << "You entered : " << arr[0] << " " << arr[1] << "  " << arr[2] << "  " << arr[3] << "  " << arr[4];

    return 0;
}
